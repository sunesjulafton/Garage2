@model Garage2.Models.Vehicle

@{
    ViewBag.Title = "Kvitto";
}




<div class="container">
    <h2>Kvitto för @ViewBag.RegNum</h2>

    <div class="well well-lg col-md-4">
        <table>
            <tr>
                <td>
                    <strong>Ankomsttid: </strong>
                </td>
                <td>
                    @ViewBag.ArrivalTime
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Utresetid: </strong>
                </td>
                <td>
                    @ViewBag.Departure
                </td>
            </tr>
            <tr>
                <td>
                    <strong>Parkeringstid: </strong>
                </td>
                <td>
                    @ViewBag.Duration
                </td>
            </tr>

        </table>
        <hr />
        <strong>Total kostnad: </strong>@ViewBag.TotalPrice
    </div>
</div>    
@Html.ActionLink("Tillbaka", "Index")
           
       




